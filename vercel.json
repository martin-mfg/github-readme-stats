{
  "buildCommand": "npm install && mkdir --parents .vercel/output/functions/isr/api.func/ && cp --recursive ./* .vercel/output/functions/isr/api.func/ && cp --recursive .vercel/output/functions/isr/api.func/_dot_vercel_copy/output/ .vercel/",
  "rewrites": [
    { "source": "/api", "destination": "/isr/api" }
  ],
  "redirects": [
    {
      "source": "/:match*",
      "destination": "/isr/:match*"
    }
  ]
}